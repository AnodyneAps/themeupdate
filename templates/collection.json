{
  "sections": {
    "breadcrumbs": {
      "type": "breadcrumbs",
      "settings": {
      }
    },
    "collection-image": {
      "type": "collection-image",
      "settings": {
        "stars_text": "{{ collection.metafields.theme.collection_banner.value }}",
        "stars_url": ""
      }
    },
    "main": {
      "type": "collection-template",
      "settings": {
        "collection_tags_enable": true,
        "collection_tags_count": true,
        "collection_multiple_tags_filtering_enable": true,
        "collection_sort_enable": true,
        "tag_selector": "button",
        "collection_layout": "grid",
        "rows": "4",
        "desktop_grid": "4",
        "mobile_grid": "2"
      }
    }
  },
  "order": [
    "breadcrumbs",
    "collection-image",
    "main"
  ]
}
